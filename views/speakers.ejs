<!DOCTYPE html>
<html>
  <head>
    <title><%= pageArtistName %> - <%= pageTitle %>  </title>
    <% include partials/structure/head.ejs %> 

  </head>
  <body id="speakerList">
    <% include  partials/content/header.ejs %> 
    <div class="container">
      <div class="row">
        <div class="col-sm-8">
          <article class="speakerslist ">
            <div class="container">
              <div class="row">

                <% speakers.forEach(speakerObj => { %>
                  <div class="col-sm-8 ">
                      <h3 class="speakerslist-title">Art in Full Bloom</h3>
                      <h5 class="speakerslist-name">with <a href="/speakers/<%= speakerObj.shortname %> "><%= speakerObj.name %> </a></h5>
                      <p class="speakerslist-info">
                        <a href="/speakers/<%= speakerObj.shortname %> ">
                            <img class="speakerslist-img img-circle pull-left" src="/images/speakers/<%= speakerObj.shortname %>_tn.jpg" alt="Photo of <%= speakerObj.name %> "></a>                        
                        </a>                                   
                      </a>
                        <%= speakerObj.summary %>                           
                      </p>
                    </div>
                <% }) %>
                  
              </div><!-- row -->
            </div><!-- container -->
          </article>
        </div><!-- primary -->
        <aside class="col-sm-4">
          <article class="sidebar">
            <h1 class="sidebar-title">Artwork on display</h1>
            <p class="sidebar-body">While you attend the conference, head over to our gallery where you can check out some of the work from our speakers.</p>
            <div class="pixgrid clearfix">

              <% artwork.forEach(element => { %>
                  <img src="/images/artwork/<%= element %> " alt="">
              <% }) %>

            </div>
          </article>
        </aside><!-- secondary -->
      </div>
    </div><!-- container -->
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.3/jquery.min.js"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/js/bootstrap.min.js" integrity="sha384-0mSbJDEHialfmuBBQP6A4Qrprq5OVfW37PRR3j5ELqxss1yVqOtnepnHVP9aJ7xS" crossorigin="anonymous"></script>

    
      <script src="/js/pixgrid.js"></script>
    

    <script src="/js/chat.js"></script>
    <script src="/reload/reload.js"></script>

  </body>
</html>
